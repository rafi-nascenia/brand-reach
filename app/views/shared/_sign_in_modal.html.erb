<%= stylesheet_link_tag 'influencers_home', media: 'all' %>

<div aria-hidden="false" class="modal new-modal fade in" id="signin">
  <div class="modal-backdrop fade in"></div>
  <div class="modal-dialog modal-md new-modal-dialog">
    <div class="modal-content new-modal-content">
      <% if user_type == 'brand' %>
          <%= render partial: 'shared/brand_log_in_sign_up_form', locals: { type: type } %>
      <% else %>
          <%= render partial: 'shared/influencer_log_in_sign_up_form', locals: { type: type } %>
      <% end %>
    </div>
  </div>
</div>
