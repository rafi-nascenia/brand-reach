<% if @success %>
    $('#success-message').html('Your information submitted successfully.When account activated we will notify you by email.');
<% else %>
    console.log('---------------- render sign up errors-----------------------' );
    $('#success-message').html('');
    $('#validation-error').html("<%= j render(:partial => 'shared/errors', :locals => { messages: @messages }) %>" );
<% end %>
