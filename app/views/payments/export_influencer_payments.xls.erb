<%- headers = ['Date Billed', 'Transaction ID', 'Amount Received(INR)', 'Payment Status'] -%>
<%= CSV.generate_line(headers, col_sep: "\t") %>
<% @influencer_payments.each do |payment| %>
    <%= CSV.generate_line([payment.billed_date.present? ? payment.billed_date.strftime('%B %d, %Y') : 'NA', sprintf('BP%05d', payment.id), payment.amount_billed, payment.status], col_sep: "\t").html_safe -%>
<% end %>