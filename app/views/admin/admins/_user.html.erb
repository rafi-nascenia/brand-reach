<tr id="user-<%= user.id %>">
  <td><%= user.first_name %></td>
  <td><%= user.last_name %></td>
  <td><%= user.email %></td>
  <td><%= user.time %></td>
  <td>
    <a href="javascript:;" class="text-success user-password-change" data-id="<%= user.email %>">
    <h5>Change Password</h5>
    </a>
  </td>
  <td>
    <% if user.active? %>
        <a href="javascript:;" class="text-red  text-danger deactivate-user" data-id="<%= user.id %>">
          <h5>DEACTIVATE</h5>
        </a>
    <% else %>
        <a href="javascript:;" class="text-success activate-user" data-id="<%= user.id %>">
          <h5>ACTIVATE</h5>
        </a>
    <% end %>
  </td>
</tr>

<script>
  $('.user-password-change')
</script>