<% if @password_reset_send_success %>
$('.success-message').html("<%= j render(:partial => 'shared/errors', :locals => { messages: @messages }) %>" );
$('.validation-error').html("");
<% else %>
$('.success-message').html("");
$('.validation-error').html("<%= j render(:partial => 'shared/errors', :locals => { messages: @messages }) %>" );
<% end %>